    function importimprintxml() {
        var data = {};
        new Ajax.Request('<?php echo $this->getUrl('productdesigner/system_config/importimprintxml/'); ?>', {
            method: 'get',
            parameters: data,
            onSuccess: function(transport) {
                alert("Imprint Area imported successfully!");
            },
            onFailure: function(transport) { 
                debugger;
                var json = transport.responseText;
                obj = JSON.parse(json);
                alert(obj.error_message);
            }
        });
    }
</script>
